<h1>Forum</h1>

<%= link_to 'Sign-up', new_user_registration_path, method: :get %> &nbsp &nbsp  
<%= link_to 'Sign-in', new_user_session_path, method: :get %> &nbsp &nbsp
<%= link_to 'Sign-out', destroy_user_session_path, method: :delete %> &nbsp &nbsp
<%= link_to 'Edit', edit_user_registration_path, method: :get %> &nbsp &nbsp
<%= link_to 'Delete', registration_path(:user), method: :delete %> &nbsp &nbsp
<%= link_to 'Forgot your password?', new_user_password_path %> &nbsp &nbsp





<% if false %>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @forums.each do |forum| %>
      <tr>
        <td><%= link_to 'Show', forum %></td>
        <td><%= link_to 'Edit', edit_forum_path(forum) %></td>
        <td><%= link_to 'Destroy', forum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Forum', new_forum_path %>
<%end%>